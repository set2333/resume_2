430\\u043D\\u0438\\u0435\\u043C \\u043E\\u0440\\u0433\\u0430\\u043D\\u0438\\u0437\\u0430\\u0446\\u0438\\u0439 \\u0432 \\u0447\\u0430\\u0441\\u0442\\u043D\\u043E\\u043C \\u043F\\u043E\\u0440\\u044F\\u0434\\u043A\\u0435. \\u041D\\u0430\\u043F\\u0438\\u0441\\u0430\\u043B \\u043D\\u0435\\u0441\\u043A\\u043E\\u043B\\u044C\\u043A\\u043E \\u043D\\u0435\\u0431\\u043E\\u043B\\u044C\\u0448\\u0438\\u0445 \\u043A\\u043E\\u043D\\u0444\\u0438\\u0433\\u0443\\u0440\\u0430\\u0446\\u0438\\u0439 \\u043D\\u0430 \\u0432\\u0441\\u0435 \\u0442\\u043E\\u0439 \\u0436\\u0435 1\\u0421 (\\u043D\\u0430\\u043F\\u0440\\u0438\\u043C\\u0435\\u0440, \\u0430\\u0432\\u0442\\u043E\\u043C\\u0430\\u0442\\u0438\\u0437\\u0438\\u0440\\u043E\\u0432\\u0430\\u043B \\u0443\\u0447\\u0435\\u0442 \\u0432 \\u043B\\u043E\\u043C\\u0431\\u0430\\u0440\\u0434\\u0435). \\u0421\\u043E\\u0432\\u0441\\u0435\\u043C \\u043D\\u0435\\u043C\\u043D\\u043E\\u0436\\u043A\\u043E \\u043F\\u043E\\u0443\\u0447\\u0430\\u0441\\u0442\\u0432\\u043E\\u0432\\u0430\\u043B \\u0432 \\u043F\\u0440\\u043E\\u0435\\u043A\\u0442\\u0435 \\u043C\\u0435\\u0434\\u0438\\u0446\\u0438\\u043D\\u0441\\u043A\\u043E\\u0439 \\u0438\\u043D\\u0444\\u043E\\u0440\\u043C\\u0430\\u0446\\u0438\\u043E\\u043D\\u043D\\u043E \\u0441\\u0438\\u0441\\u0442\\u0435\\u043C\\u044B (\\u043F\\u0440\\u043E\\u0435\\u043A\\u0442 \\u043D\\u0430 C#, \\u043A\\u0441\\u0442\\u0430\\u0442\\u0438, \\u0435\\u0434\\u0438\\u043D\\u0441\\u0442\\u0432\\u0435\\u043D\\u043D\\u044B\\u0439 \\u043F\\u0440\\u043E\\u0435\\u043A\\u0442 \\u0432 \\u0440\\u0435\\u0433\\u0438\\u043E\\u043D\\u0435, \\u043D\\u0435 \\u043D\\u0430 1\\u0421), \\u043D\\u043E \\u0438\\u0437-\\u0437\\u0430 \\u0432\\u043D\\u0443\\u0442\\u0440\\u0435\\u043D\\u043D\\u0438\\u0445 \\u0440\\u0430\\u0437\\u043D\\u043E\\u0433\\u043B\\u0430\\u0441\\u0438\\u0439 \\u0432 \\u0440\\u0443\\u043A\\u043E\\u0432\\u043E\\u0434\\u0441\\u0442\\u0432\\u0435, \\u043F\\u0440\\u043E\\u0435\\u043A\\u0442 \\u043F\\u0440\\u0438\\u0448\\u043B\\u043E\\u0441\\u044C \\u043F\\u043E\\u043A\\u0438\\u043D\\u0443\\u0442\\u044C.\"), __jsx(\"p\", {\n  __source: {\n    fileName: _jsxFileName,\n    lineNumber: 148\n  },\n  __self: this\n}, \"\\u0411\\u044B\\u043B\\u043E \\u043D\\u0435\\u0441\\u043A\\u043E\\u043B\\u044C\\u043A\\u043E \\u0441\\u0432\\u043E\\u0438\\u0445 \\u0441\\u0430\\u0439\\u0442\\u043E\\u0432. \\u0425\\u043E\\u0442\\u0435\\u043B \\u0437\\u0430\\u0440\\u0430\\u0431\\u043E\\u0442\\u0430\\u0442\\u044C \\u043D\\u0430 \\u043A\\u043E\\u043D\\u0442\\u0435\\u043A\\u0441\\u0442\\u043D\\u043E\\u0439 \\u0440\\u0435\\u043A\\u043B\\u0430\\u043C\\u0435. \\u0414\\u043E\\u0445\\u043E\\u0434 \\u0431\\u044B\\u043B, \\u043D\\u043E \\u043D\\u0430\\u0441\\u0442\\u043E\\u043B\\u044C\\u043A\\u043E \\u043D\\u0435\\u0431\\u043E\\u043B\\u044C\\u0448\\u043E\\u0439, \\u0447\\u0442\\u043E \\u043F\\u0440\\u043E\\u0435\\u043A\\u0442\\u044B \\u043F\\u0440\\u0438\\u0448\\u043B\\u043E\\u0441\\u044C \\u0441\\u0432\\u0435\\u0440\\u043D\\u0443\\u0442\\u044C.\"));\n\nexport default ContentWork;","map":{"version":3,"sources":["/home/set/Projects/resume_2/Components/Contents/ContentWork.jsx"],"names":["React","ListItem","ContentWork"],"mappings":";;AAAA,OAAOA,KAAP,MAAkB,OAAlB;AACA,OAAOC,QAAP,MAAqB,6BAArB;;AAEA,MAAMC,WAAW,GAAG,MAClB,4BACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,iDADF,EAEE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,ksCAFF,EAOE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,mHAPF,EAQE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,yCADF,CADF,EAIE,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,UADF,CAJF,EAOE,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SADF,CAPF,EAUE,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,UADF,EAEG,GAFH,uKAVF,EAeE,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,2BADF,CAfF,EAkBE,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,aADF,CAlBF,EAqBE,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,oFADF,EAEG,GAFH,8DArBF,EA0BE,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,kFADF,EAEG,GAFH,wIA1BF,CARF,EAwCE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uJAxCF,EAyCE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SADF,EAEG,GAFH,yQADF,EAME,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SADF,EAEG,GAFH,8FANF,EAWE,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,WADF,EAEG,GAFH,kFAXF,EAgBE,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,YADF,EAEG,GAFH,6KAhBF,EAqBE,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,SADF,EAEG,GAFH,mOArBF,EA0BE,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,eADF,EAEG,GAFH,oJA1BF,CAzCF,EAyEE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,wGAzEF,EA0EE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,YADF,CADF,EAIE,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,YADF,CAJF,EAOE,MAAC,QAAD;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,GACE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,aADF,CAPF,CA1EF,EAqFE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,olBArFF,EAyFE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,gjBAzFF,EA6FE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,6lCA7FF,EAkGE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,uiCAlGF,EAuGE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,ixGAvGF,EAiHE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,o2DAjHF,EAwHE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,q3FAxHF,EAiIE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,msEAjIF,EAwIE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,klEAxIF,EA+IE;AAAA;AAAA;AAAA;AAAA;AAAA;AAAA,ksBA/IF,CADF;;AAuJA,eAAeA,WAAf","sourcesContent":["import React from 'react';\nimport ListItem from '../OtherComponents/ListItem';\n\nconst ContentWork = () => (\n  <>\n    <h1>О работе</h1>\n    <p>\n      Начну, пожалуй, со стека технологий. Его я разобью на три списка. В первом будут те языки и\n      технологии, которые я использовал в своей рабочей деятельности. Во втором те, с которыми я\n      ознакомился просто из интереса. В третьем планы на будущее.\n    </p>\n    <h2>Основные технологии</h2>\n    <ul>\n      <ListItem>\n        <b>JavaScript (Node.js, React, jQuery)</b>\n      </ListItem>\n      <ListItem>\n        <b>HTML</b>\n      </ListItem>\n      <ListItem>\n        <b>CSS</b>\n      </ListItem>\n      <ListItem>\n        <b>Java</b>\n        {' '}\n        (разрабатывал приложение под Android)\n      </ListItem>\n      <ListItem>\n        <b>SQL (PostGree, MySQL)</b>\n      </ListItem>\n      <ListItem>\n        <b>MongoDB</b>\n      </ListItem>\n      <ListItem>\n        <b>1С Предприятие 7.7</b>\n        {' '}\n        (годы жизни)\n      </ListItem>\n      <ListItem>\n        <b>1С Предприятие 8</b>\n        {' '}\n        (многие-многие годы жизни)\n      </ListItem>\n    </ul>\n    <h2>Дополнительные технологии</h2>\n    <ul>\n      <ListItem>\n        <b>PHP</b>\n        {' '}\n        (использовал для одного сайта на небольшом уровне)\n      </ListItem>\n      <ListItem>\n        <b>C++</b>\n        {' '}\n        (на нем изучал ООП)\n      </ListItem>\n      <ListItem>\n        <b>Basic</b>\n        {' '}\n        (это ещё в школе)\n      </ListItem>\n      <ListItem>\n        <b>Delphi</b>\n        {' '}\n        (в свое время знатно похулиганил)\n      </ListItem>\n      <ListItem>\n        <b>MQL</b>\n        {' '}\n        (приходилось писать ботов для Forex-терминалов)\n      </ListItem>\n      <ListItem>\n        <b>Assembler</b>\n        {' '}\n        (узнавал принципы работы ПК)\n      </ListItem>\n    </ul>\n    <h2>В ближайших планах</h2>\n    <ul>\n      <ListItem>\n        <b>Python</b>\n      </ListItem>\n      <ListItem>\n        <b>Vue.js</b>\n      </ListItem>\n      <ListItem>\n        <b>Angular</b>\n      </ListItem>\n    </ul>\n    <p>\n      Теперь небольшое лирическое отступление. Так как у меня нет на руках трудовой книжки, то все\n      даты будут примерными.\n    </p>\n    <p>\n      Итак. Началась моя ИТ-карьера в организации под названием Почта России. Это было в период с\n      мая 2007г. по август 2008г.\n    </p>\n    <p>\n      Основной задачей было обслуживание программы WinPost, которую использовали операторы в\n      отделениях почтовой связи. Там я писал свои первые SQL-скрипты, которые позволяли быстро и без\n      ошибок настраивать новые рабочие места.\n    </p>\n    <p>\n      С августа 2008г. по февраль 2009г. я работал в Кызылской ТЭЦ электромонтёром РЗА. Напрямую с\n      ИТ работа связана не была, но удалось поучаствовать в установке системы пожаротушения, которая\n      была компьютеризирована.\n    </p>\n    <p>\n      Далее, с февраля 2009г. и по декабрь 2011г. я трудился на должности программиста в Тувинской\n      горнорудной компании. В начале основной платформой была 1С Предприятие версии 7.7. Позже\n      перешли на версию 8. Здесь мне довелось активно поучаствовать в автоматизации процессов\n      предприятия. Я писал модули для расчёта зарплаты белазистов и бульдозеристов. Написал\n      небольшую конфигурацию для учета ходок, которая заменила очень большой и страшный excel-файл.\n      Участвовал в настройке электронных весов, которые завешивают автомобили. Так же производил их\n      интеграцию с 1С. Создавал модуль учета прибывшего на погрузку транспорта, с помощью все той же\n      1С и сканера штрих кодов.\n    </p>\n    <p>\n      Следующим моим рабочим местом был ООО Альянс (с января 2012г. по декабрь 2013г.). Эта\n      организация - официальный партнер 1С. Там вся моя работа была связанна с этой платформой.\n      Начиная от установки и обновления конфигураций, заканчивая разработкой отчетов и обработок под\n      нужды клиентов. Ну и, конечно же, консультации, хотя это уже не столько ИТ-шные навыки,\n      сколько бухгалтерские.\n    </p>\n    <p>\n      С января 2014г. и по май 2017г. я работал в ООО Лаим. Небольшая организация, занимавшаяся\n      разработкой чего угодно. Мы делали и системы документооборота, на базе все той же 1С. Писали\n      приложение под Android на Java. Программировали Forex-терминалы. Писали игры на GameMaker.\n      Разрабатывали систему учета топлива на дизельных электростанциях в отдаленных районах\n      республики (смесь 1С, JavaScript, Node.js).Это был, наверное, самый интересный этап моей\n      карьеры. Там я получил навыки командной работы. Узнал кучу новых технологий. Наконец-то понял\n      всю суть ООП. Но по экономическим обстоятельствам пришлось менять место работы.\n    </p>\n    <p>\n      С мая 2017г. и по сегодняшний день я работаю в Управлении образованием Каа-Хемского кожуна.\n      Эта организация занимается централизованным бухучетом примерно 30 организаций. То есть простор\n      для автоматизации. Многие задачи уже решены, но имеется куча эникейских функций, которые\n      отнимают кучу времени. Хотелось бы отойти от этого, ведь я уверен, что мой потенциал будет,\n      мягко говоря, повыше, чем установка ключей ЭЦП и замена бумаги в принтере.\n    </p>\n    <p>\n      Года с 2010, параллельно с основной деятельностью, занимался обслуживанием организаций в\n      частном порядке. Написал несколько небольших конфигураций на все той же 1С (например,\n      автоматизировал учет в ломбарде). Совсем немножко поучаствовал в проекте медицинской\n      информационно системы (проект на C#, кстати, единственный проект в регионе, не на 1С), но\n      из-за внутренних разногласий в руководстве, проект пришлось покинуть.\n    </p>\n    <p>\n      Было несколько своих сайтов. Хотел заработать на контекстной рекламе. Доход был, но настолько\n      небольшой, что проекты пришлось свернуть.\n    </p>\n  </>\n);\n\nexport default ContentWork;\n"]},"metadata":{},"sourceType":"module"}